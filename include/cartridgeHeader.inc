INCLUDE "hardware.inc"


DEF GAME_NAME EQUS "RUNMAN"
DEF GAME_NAME_PADDING = 11 - STRLEN("{GAME_NAME}")
DEF GAME_DEV EQUS "NEP"
DEF GAME_DEV_PADDING = 4 - STRLEN("{GAME_DEV}")
DEF CART_GBC_COMPAT = CART_COMPATIBLE_DMG
DEF NEW_LICENSEE_CODE = $0000
DEF CART_SGB_COMPAT = CART_INDICATOR_GB
DEF CART_MAPPER_TYPE = CART_ROM_MBC1_RAM_BAT
DEF CART_ROM_SIZE = CART_ROM_32KB
DEF CART_SRAM_SIZE = CART_SRAM_8KB
DEF CART_DESTINATION = CART_DEST_NON_JAPANESE
DEF CART_OLD_LICENSEE_CODE = $00
DEF GAME_VERSION = $00
DEF HEADER_CHECKSUM = $00
DEF GLOBAL_CHECKSUM = $0000

MACRO HEADER
    NINTENDO_LOGO
	db "{GAME_NAME}"
	ds GAME_NAME_PADDING, 0
	db "{GAME_DEV}"
	ds GAME_DEV_PADDING, 0
	db CART_COMPATIBLE_DMG
	dw NEW_LICENSEE_CODE
	db CART_SGB_COMPAT
	db CART_MAPPER_TYPE
	db CART_ROM_SIZE
	db CART_SRAM_SIZE
	db CART_DESTINATION
	db CART_OLD_LICENSEE_CODE
	db GAME_VERSION
	db HEADER_CHECKSUM
	dw GLOBAL_CHECKSUM
ENDM
